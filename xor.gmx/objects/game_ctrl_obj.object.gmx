            <string>distancebetween-=(sprite_height*image_yscale)
if distancebetween&lt;sprite_height
{
	audio_stop_sound(electricEffect_snd)
	electricEffectStarted=false
	confirmed=1
	//This little baby checks if all values were different
	for (i=0;i&lt;=length;i++)
	{
		if not (((fallingbits&gt;&gt;i)&amp;1)^((recievingbits&gt;&gt;i)&amp;1))
		{
			part=instance_create(x+((sprite_width*image_xscale)*((length/2)-i+0.5)),y+((sprite_height*image_yscale)/2),brokenmistake_obj)
			part.image_index=((fallingbits&gt;&gt;i)&amp;1)^((recievingbits&gt;&gt;i)&amp;1)
		}
		confirmed= confirmed&amp;(((fallingbits&gt;&gt;i)&amp;1)^((recievingbits&gt;&gt;i)&amp;1))
		
	}
	
	spawnedItemAlpha=0
	
	if confirmed
	{
		//If we've used 31 bits let's not make that higher in case
		//we decide to compile for 32-bit systems.
		//Instead, let's make things a bit harder by speeding them up
		if length&lt;31
		{
			length++
		}
		//If fallblock delay is lower than 1 the timer will just turn off
		//so we want to prevent that
		else if fallblockdelay&gt;0 
		{
			fallblockdelay--
		}
		//If the player gets a correct combo increase the level
		level+=1
	}
	//RESET!
	//The value of operand1
	fallingbits=1+irandom(power(2,length)-1)
	
	//The value of operand2
	recievingbits=1+irandom(power(2,length)-1)
	
	distancebetween=(sprite_height*image_yscale)*8
	
	//This lets us scale as the game goes on so we can fit everything on the page
	//image_xscale=1/length
	//image_yscale=1/length
	//
}
alarm[0]=fallblockdelay</string>
